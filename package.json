{
  "name": "gulp-boilerplate",
  "version": "0.0.1",
  "description": "Gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickleefly/gulp-boilerplate.git"
  },
  "//": [
    "The 'plugin' example makes that file requireable with `require('plugin')`,",
    "and available to browserify-shim as 'plugin' on line 30."
  ],
  "browser": {
    "plugin": "./web/javascript/vendor/jquery-plugin.js",
    "bootstrap": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap.js",
    "jquery": "./node_modules/jquery/dist/jquery.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "$",
    "jquery-ui": {
      "depends": "jquery:$"
    },
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    },
    "bootstrap": {
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "bootstrap-sass": "~3.3.4",
    "browser-sync": "^2.9.12",
    "browserify": "^12.0.1",
    "browserify-shim": "^3.8.2",
    "del": "^1.1.1",
    "font-awesome": "^4.3.0",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-changed": "^1.1.1",
    "gulp-filesize": "0.0.6",
    "gulp-iconfont": "^1.0.0",
    "gulp-imagemin": "^2.2.1",
    "gulp-minify-css": "~0.5.1",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^2.1.0",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-swig": "^0.7.4",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.4",
    "node-static": "^0.7.7",
    "vinyl-buffer": "^1.0.0",
    "vinyl-paths": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^2.4.0"
  },
  "dependencies": {
    "factor-bundle": "^2.5.0",
    "jquery": "~2.1.0",
    "jquery-ui": "^1.10.5"
  }
}
